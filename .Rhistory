mtext(text = types[i], outer = FALSE, side = 3, line = 0.3)
mtext(text = paste0("(", letters[i], ")"), outer = FALSE, side = 3, at = 0.5, line = 0.3)
mtext(text = "Ivlev's E", outer = TRUE, side = 2, line = 1.5)
}
dev.off()
